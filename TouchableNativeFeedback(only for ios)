import React from 'react';
import { StyleSheet,TouchableNativeFeedback,View,Text, SafeAreaView} from'react-native';
export default function App (){
  
    

    return (
      

      <SafeAreaView style = {styles.container}>
      <SafeAreaView>
      <Text numberOfLines={1} >Hello world from react  </Text>
      <TouchableNativeFeedback  onPress={()=> console.log("Image Tapped")}>              //Remember this only works on IOS , not supportive in Andriod
        <View style={{width:200,height:70,backgroundColor:'dodgerblue'}}/>  
              </TouchableNativeFeedback>
     

             
      </SafeAreaView>
      
      </SafeAreaView>

    );
  }



const styles = StyleSheet.create({
  container: {
    flex: 1,
    backgroundColor: '#fff',
    justifyContent: "center",
    alignItems : "center"
  },
});

